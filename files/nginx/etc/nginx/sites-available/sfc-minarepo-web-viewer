
server {
  listen 80;
  listen [::]:80 ipv6only=on;

  server_name sfc-minarepo.ht.sfc.keio.ac.jp;

  location = /favicon.ico {
    root /usr/local/sfc-minarepo-web-viewer/app/sfc-minarepoviewer/static;
  }

  location /static {
    alias /usr/local/sfc-minarepo-web-viewer/app/sfc-minarepoviewer/static;
    gzip on;
    gzip_types text/plain application/javascript application/x-javascript text/css;
  }

  location / {
    proxy_set_header Host $host;
    proxy_set_header X-Real-IP $remote_addr;
    proxy_pass http://127.0.0.1:3788;
  }
}
